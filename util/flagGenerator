$vhdlData = array();

foreach ($flag as $rowIndex => $row) {
	$startIndex = 0;
	$endIndex = 239;

	for ($i = 0, $last = "+"; $i < strlen($row); $last = $row[$i], ++$i) {
		if ($row[$i] == " " && $last == "+") {
			$startIndex = $i;
		}

		if ($row[$i] == "+" && $last == " ") {
			$endIndex = $i;
		}
	}

	$vhdlData[] = "(to_unsigned($startIndex, 10), to_unsigned($endIndex, 10))";
}

echo "--\n";
echo "-- AUTO-GENERATED by util/flagGenerator\n";
echo "--\n";
echo "\n";
echo "constant FLAG_ROWS : integer := 160;\n";
echo "constant FLAG_COLUMNS : integer := 240;\n";
echo "\n";
echo "subtype index is unsigned(9 downto 0);\n";
echo "type indexPair is array(0 to 1) of index;\n";
echo "type imageData is array(0 to 159) of indexPair;\n";
echo "constant IMAGE : imageData := ( " . implode(", ", $vhdlData) . " );\n";
echo "\n";
echo "--\n";
echo "-- END AUTO-GENERATED\n";
echo "--\n";

